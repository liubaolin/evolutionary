    <bean id="springOutputHelper" class="com.baolinliu.output.helper.SpringOutputHelper">
        <property name="outputGenerator" ref="JsonOutputGenerator"/>
    </bean>

    <bean id="JsonOutputGenerator" class="com.baolinliu.output.impl.JsonOutputGenerator"></bean>

</beans>